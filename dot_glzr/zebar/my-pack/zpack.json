{
  "$schema": "https://github.com/glzr-io/zebar/raw/v3.1.1/resources/zpack-schema.json",
  "name": "my-pack",
  "version": "0.0.0",
  "description": "",
  "tags": [],
  "previewImages": [
    "./resources/preview-image-1.png"
  ],
  "repositoryUrl": "",
  "widgets": [
    {
      "name": "zebar-glazewm",
      "htmlPath": "zebar-glazewm/dist/index.html",
      "zOrder": "normal",
      "shownInTaskbar": false,
      "focused": false,
      "resizable": false,
      "transparent": true,
      "includeFiles": [
        "zebar-glazewm/dist/**"
      ],
      "caching": {
        "defaultDuration": 604800,
        "rules": []
      },
      "privileges": {
        "shellCommands": []
      },
      "presets": [
        {
          "name": "default",
          "anchor": "top_center",
          "offsetX": "0px",
          "offsetY": "0px",
          "width": "100%",
          "height": "40px",
          "monitorSelection": {
            "type": "all"
          },
          "dockToEdge": {
            "enabled": true,
            "edge": null,
            "windowMargin": "0px"
          }
        }
      ]
    },
    {
      "name": "starter-bar",
      "htmlPath": "starter-bar/vanilla.html",
      "zOrder": "normal",
      "shownInTaskbar": false,
      "focused": true,
      "resizable": false,
      "transparent": true,
      "includeFiles": [
        "starter-bar/**"
      ],
      "caching": {
        "defaultDuration": 604800,
        "rules": []
      },
      "privileges": {
        "shellCommands": []
      },
      "presets": [
        {
          "name": "default",
          "anchor": "top_left",
          "offsetX": "0px",
          "offsetY": "0px",
          "width": "100%",
          "height": "40px",
          "monitorSelection": {
            "type": "all"
          },
          "dockToEdge": {
            "enabled": true,
            "edge": null,
            "windowMargin": "0px"
          }
        }
      ]
    }
  ]
}
